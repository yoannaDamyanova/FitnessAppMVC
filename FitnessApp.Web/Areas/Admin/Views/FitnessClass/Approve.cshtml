@model IEnumerable<FitnessClassServiceModel>

@{
    ViewBag.Title = "Approve";
}
<h2 class="text-center">@ViewBag.Title</h2>

<hr />
<h4>Fitness classes to approve</h4>
@if (!Model.Any())
{
    <h2 class="text-center">You have no Fitness classes to approve!</h2>
}
<div class="row">
    @foreach (var fitnessClass in Model)
    {
        @await Html.PartialAsync("~/Areas/Admin/Views/Shared/_ApproveFitnessClassPartial.cshtml", fitnessClass)
    }
</div>